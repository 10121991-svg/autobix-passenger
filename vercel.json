{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "headers": {
        "Content-Type": "application/json"
      },
      "dest": "/manifest.json"
    },
    {
      "src": "/favicon.ico",
      "headers": {
        "Content-Type": "image/x-icon"
      },
      "dest": "/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build"
}